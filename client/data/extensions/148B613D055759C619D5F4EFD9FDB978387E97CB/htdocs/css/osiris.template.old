    background-image: url("data:image/png;base64,");
}

ul.arrow {
    list-style: none outside none;
    margin: 0;
    padding: 0;
}

ul.arrow > li {
    background: url("data:image/png;base64,") no-repeat scroll 0 0 transparent;
    margin-top: 5px;
    padding: 5px 0 0 30px;
}

.mod-box-black ul.arrow > li, .mod-box-color ul.arrow > li, #bottom-b .mod-line ul.arrow > li {
    background-image: url("data:image/png;base64,");
}

ul.zebra > li, table.zebra tbody td {
    border-bottom: 1px solid #DDDDDD;
}

ul.zebra > li:first-child, table.zebra tbody tr:first-child td {
    border-top: 1px solid #DDDDDD;
}

ul.zebra > li.odd {
    background: none repeat scroll 0 0 #FAFAFA;
}

ul.zebra > li:nth-of-type(2n+1) {
    background: none repeat scroll 0 0 #FAFAFA;
}

.mod-box-black ul.zebra > li, .mod-box-black ul.zebra > li:first-child, #bottom-b .mod-line ul.zebra > li, #bottom-b .mod-line ul.zebra > li:first-child {
    border-color: #444444;
}

.mod-box-black ul.zebra > li.odd, #bottom-b .mod-line ul.zebra > li.odd {
    background-color: #555555;
}

.mod-box-black ul.zebra > li:nth-of-type(2n+1), #bottom-b .mod-line ul.zebra > li:nth-of-type(2n+1) {
    background-color: #555555;
}

.mod-box-color ul.zebra > li, .mod-box-color ul.zebra > li:first-child {
    border: medium none;
}

