/* 
Osiris CSS Template.
This template are processed by OMS, that generate two file: "structure.css" and "style.less". And "style.css" by compiling less file.
"style.less" can be copied by skins, to generate a different theme.

LESS format: http://lesscss.org/
Functions of dotless used here: https://github.com/dotless/dotless/wiki/Function-reference
*/

/* ======================================== */
/*   Elements Styles						*/
/* ======================================== */

html {
	overflow-x: auto; /* fixes MSIE scrollbar bug DO NOT REMOVE, has no effect in Mozilla, or Opera */	
	background-color: @os-color-bg;
}

body, .os_root {
	margin: 0px;
	padding: 0px;
	color: @os-color-fg;
	background-color: @os-color-bg;
	font-family: @os-font-family;
	font-size: @os-font-size;
	text-align: left;		
}

/* Prevent dotted border in Firefox */
a:active { outline: none; }
a:focus { -moz-outline-style: none; }

a:link,
a:visited,
a:active,
a:focus {	
	background-color: transparent;
	color: @os-color-link;	
	
	/*
	text-decoration: none;
	padding-bottom: 0px;
	background-image: url("/htdocs/images/link.gif");
	background-repeat: repeat-x;
	background-attachment: scroll;
	background-position: left bottom;	
	*/

	.os-transition();
}

a:hover {
	background-color: transparent;
	color: lighten(@os-color-link, 20%);
	
	/*
	text-decoration: none;	
	padding-bottom: 0px;
	background-image: url("/htdocs/images/link_flash.gif");
	background-repeat: repeat-x;
	background-attachment: scroll;
	background-position: left bottom;
	*/
}

table {
}

table, tr, td, th {
	border: 0px;
	padding: 0px;
	margin: 0px;
	background: transparent;
	border-collapse:collapse;
	border-spacing:0px;
	vertical-align: top;
	text-align: left;
}

form {
	margin: 0;
	padding: 0;
	display: inline;
}

img {
	border: 0;
	vertical-align: baseline;
}

h1, h2, h3, h4, h5, h6 {	
	margin-top:12px;
	margin-bottom:3px;
	padding-bottom:2px;
	color: @os-color-fg;
	text-shadow: 1px 1px 2px @os-color-fg;
	background-color:Transparent;
	border-bottom: 1px solid lighten(@os-color-bg, (@os-color-delta * -60%));
}

h1 {
	color: @os-color-a;
	font-size: 1.5em;
	line-height: 1.5em;
}

h2 {
	color: @os-color-a;
	font-size: 1.3em;
	line-height: 1.3em;
}

h3 {
	color: @os-color-a;
	font-size: 1.2em;
	line-height: 1.2em;
}

h4 {
	font-size: 1.15em;
	line-height: 1.15em;
}

h5 {
	font-size: 1.125em;
	line-height: 1.125em;
}

h6 {
	font-size: 1.1em;
	line-height: 1.1em;
}

div {
	margin: 0px; 	
	padding: 0px;
}

input, select, textarea {		
	padding: 2px;
	margin: 2px;
	border: 1px solid @os-color-gray7;
	background-color: @os-color-input-bg;
	color: @os-color-input-fg;	
	font-size: 12px;
	vertical-align: middle;	
	
	border-radius: 0.3em;	
	-webkit-border-radius: 0.3em;
}

input:focus, select:focus, textarea:focus {
	.os-focus();
	/*
	background-color: @os-color-focus;
	border:1px solid @os-color-a;
	*/
}

input, select, textarea {
	font-family: @os-font-family;
	font-size: 12px;
}

fieldset {
	padding-bottom: 5px;
}

legend {
	color: @os-color-fg;
}

textarea 
{
	width: 98%;
}

ul, ol, dl {
    padding-left: 30px;
}

ul {
	list-style-image: url("/htdocs/images/oml/list_bullet.png");
}

li {
	margin-top:8px;
}

pre {
 white-space: pre-wrap;       /* css-3 */
 white-space: -moz-pre-wrap;  /* Mozilla, since 1999 */
 white-space: -pre-wrap;      /* Opera 4-6 */
 white-space: -o-pre-wrap;    /* Opera 7 */
 word-wrap: break-word;       /* Internet Explorer 5.5+ */
}

ins {
	background-color: #EAFFEA;
}

del {
	background-color: #FFEAEA;
}

q {
	font-style: italic;
}

mark {
	background-color: @os-color-highlight;
}

/* ========================================================================================================== */
/*   Content Styles																							  */
/*   It's raccomended to final users to use this styles in their contents, and never the other "os_*" styles. */
/* ========================================================================================================== */

.transitions-all {
	.os-transition();
}

ul.blank, ul.line, ul.check, ul.zebra {
    list-style: none outside none;
    margin: 0;
    padding: 0;
}

ul.space li {
    margin-top: 15px;
}

ul.space li:first-child {
    margin-top: 0;
}

ul.line > li {
    margin-top: 5px;
    padding-top: 5px;
}

ul.line > li:first-child {
    margin-top: 0;
    padding-top: 0;
}

ul.check > li {
    margin-top: 5px;
}

ul.check > li:first-child {
    margin-top: 0;
}

ul.zebra > li {
    padding: 5px;
}

ul.line > li > a:first-child, ul.check > li > a:first-child {
    display: block;
}

table.zebra th {
    font-size: 16px;
    font-weight: normal;
    text-align: left;
}

table.zebra th, table.zebra td {
    padding: 5px;
}

table.zebra tbody, table.zebra tfoot {
    font-size: 12px;
}

table.zebra td * {
    vertical-align: middle;
}

table.zebra tfoot {
    font-style: italic;
}

table.zebra caption {
    font-size: 11px;
    font-style: italic;
    text-align: left;
}

dl.separator {
    overflow: hidden;
    padding: 0;
}

dl.separator dt {
    clear: both;
    float: left;
    padding-right: 10px;
    width: 160px;
}

dl.separator dd {
    margin-left: 170px;
    padding-left: 20px;
}

.box-content, .box-info, .box-warning, .box-error, .box-hint, .box-note {
    background-color: @os-color-gray9;
	background-repeat: no-repeat;
	background-position: 5px, 5px;
    border: 1px solid @os-color-gray7;
    border-radius: 3px 3px 3px 3px;
    box-shadow: 0 1px 2px rgba(0, 0, 0, 0.08);
	min-height: 18px;
	margin: 10px;
    padding: 10px;
    text-align: justify;
	padding-left: 50px;
}

.box-content {
    padding: 20px;
}

.box-info {
    background-image: url("/htdocs/images/icons/32x32/info.png");
	background-color: @os-color-info;
	border-color: lighten(@os-color-info, (@os-color-delta * -20%));    
    color: lighten(@os-color-info, (@os-color-delta * -60%));    
}

.box-warning {
    background-image: url("/htdocs/images/icons/32x32/warning.png");
	background-color: @os-color-warning;
	border-color: lighten(@os-color-warning, (@os-color-delta * -20%));    
    color: lighten(@os-color-warning, (@os-color-delta * -60%));    
}

.box-error {
    background-image: url("/htdocs/images/icons/32x32/error.png");
	background-color: @os-color-error;	
	border-color: lighten(@os-color-error, (@os-color-delta * -20%));    
    color: lighten(@os-color-error, (@os-color-delta * -60%));    
}

.box-hint {
    background-image: url("/htdocs/images/icons/32x32/hint.png");
	background-color: @os-color-hint;
	border-color: lighten(@os-color-hint, (@os-color-delta * -20%));    
    color: lighten(@os-color-hint, (@os-color-delta * -60%));    
}

.box-note {
    background-image: url("/htdocs/images/icons/32x32/note.png");
	background-color: @os-color-note;
	border-color: lighten(@os-color-note, (@os-color-delta * -20%));    
    color: lighten(@os-color-note, (@os-color-delta * -60%));    
}

p.dropcap:first-letter {
    display: block;
    float: left;    
    font-size: 280%;
    margin: 7px 4px 0 0;
}

